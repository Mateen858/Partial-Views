
@{
    ViewBag.Title = "Index";
    List<Test> test = ViewBag.abcList;
}

<h2>Index</h2>

<div class="container">
    <div class="row">
        <div class="col-md-6">
            <h3>Static Partial View Rendering</h3>
            @Html.Partial("_MyPartialView")
        </div>
        <div class="col-md-6">
            <h3>Dynamic Partial VIew Rendering</h3>
            @foreach (var t in test)
            {
                @Html.Partial("_DynamicPartialView", t);
            }

        </div>
    </div>

</div>